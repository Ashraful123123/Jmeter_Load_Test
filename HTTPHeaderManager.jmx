            <stringProp name="script">import java.io.FileWriter;
import java.io.IOException;
FileWriter writer = null;
//Filewriter name akta class define korte hobe jar object r nam hocche --writer--
//jehetu shurute amra kichu likhbo na ,shudu initialize korlam,tai r value null
String location = &quot;C:\\Users\\Abir\\Downloads\\apache-jmeter-5.4.3\\apache-jmeter-5.4.3\\bin\\Customer_Load_Test\\customers.csv&quot;;
//ami jei file a data save korte cacchi shei file tar location
log.info(&quot;=============Started wrting Data to CSV file=======&quot;);
//kono kichu print korte caile log.nfo
try {
	//aikhane writer hocche object r FileWriter constructor
	//r moddhe duita parameter pass korechi akta hocche
	//file location customer.csv
	writer = new FileWriter(location, true);
	//true mane bar bar she aki file kei use korbe,file take apend korbe
	String custId = vars.get(&quot;id&quot;);
	//amader jei response ta asche response tta get korar jonno vars.get
	//use korechi then variable r nam diyechi
	String name = vars.get(&quot;name&quot;);
	String email = vars.get(&quot;email&quot;);
	String address=vars.get(&quot;address&quot;);
	//vars.get r maddhome variable theke data ta retrive korlam
	//abong String variable name,email r moddhe rakhlam
	String phone = vars.get(&quot;phone_number&quot;);
	writer.append(custId+&quot;,&quot;+name+&quot;,&quot;+email+&quot;,&quot;+address+&quot;,&quot;+phone);
	//writer hocche object then append hocche mrthod name
	//aktar por akta apend korbo,read korbe
	//tarpor ami csv file a data pass kortesi,ami variable guloke pass korbo
	//prothom data CustID,tarpor Concate korsi , r sathe,karonCSV mane Comma separated value
	//value gulo comma die separate kora thakbe
	//tar jonno prottekta separation r porei comma die rakhsi
	writer.append(&apos;\n&apos;);
	//akta full data lekhar por second data ta new line a likhbe
	//prottektar mazhe akta kore new line create korbe
	log.info(custId+&quot; &quot;+name+&quot; &quot;+email+&quot; &quot; +phone+&quot; &quot;+phone+&quot; &quot;);
	//log a ami data gulo dekhbo	
	log.info(&quot;CSV file is created... &quot;);
	writer.flush();
	//data guloke likhbe save korbe abong close kore dibe

} catch (IOException e) {
	e.printStackTrace();
}

writer.close();
//abong close kore dibe
</stringProp>
