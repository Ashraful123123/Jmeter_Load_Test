            <stringProp name="script">import org.apache.http.client.methods.HttpGet
import org.apache.http.impl.client.HttpClientBuilder
import org.apache.http.util.EntityUtils
import groovy.json.JsonSlurper

log.info(&apos;----- Calling user info generating API -----&apos;)
def httpClient = HttpClientBuilder.create().build()
//https://randomuser.me/api/ ai api take call kortesi browser r maddhome,postman,jmeter r maddhome
//aigula chara call korte parbo na.
//aiguloke Rest Client bole,aitar kaj jkono RESTapi ba je kono url ke call kora
//to akhon amara jehetu jmeter r through call korbo tai amader akta client lagbe
//Client bananor jonno HttpClientBuilder class korar karone,code nij theke custom client banaiya nibe
//Build kore httpClient variable a rakhbe
//def thakle bujhe jai kon type r datatype
def httpGet = new HttpGet(&quot;https://randomuser.me/api/&quot;)
def httpResponse = httpClient.execute(httpGet)

def responseData = EntityUtils.toString(httpResponse.getEntity())

log.info(&apos;----- Response -----&apos;)
log.info(responseData)

//Akhon kaj hocche Respontake Access Kora
slurperresponse = new JsonSlurper().parseText(responseData)
log.info(&apos;----- Set to environment variables-----&apos;)
vars.put(&quot;name&quot;,slurperresponse.results[0].name.first)
//aikhane variable name r moddhe value guloke store korechi
vars.put(&quot;email&quot;,slurperresponse.results[0].email)
//pre processor a amra --vars.put r maddhome variable a data gulo store korechi
//variable hocche name,email,address,phone number
//aguloke tarpor ami jmeter r moddhe get korechi
//CREATE CUSTOMER request r body r moddhe---&quot;name&quot; : &quot;${name}&quot;,&quot;email&quot; : &quot;${email}&quot;
//---------------------//
//kintu jokhon ami code r moddhe likhte jabo
vars.put(&quot;address&quot;,slurperresponse.results[0].location.city)
vars.put(&quot;phone_number&quot;,slurperresponse.results[0].phone)
//
</stringProp>
